{
    "provider": {
        "name": "productListProvider"
    },
    "consumer": {
        "name": "consumerA"
    },
    "interactions": [
        {
            "description": "user id",
            "request": {
                "method": "GET",
                "path": "/products/1"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json; charset=UTF-8"
                },
                "body": {
                    "productName": "iphoneX"
                },
                "matchingRules": {
                    "body": {
                        "$.productName": {
                            "matchers": [
                                {
                                    "match": "regex",
                                    "regex": ".*"
                                }
                            ],
                            "combine": "AND"
                        }
                    },
                    "header": {
                        "Content-Type": {
                            "matchers": [
                                {
                                    "match": "regex",
                                    "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "get products"
                }
            ]
        },
